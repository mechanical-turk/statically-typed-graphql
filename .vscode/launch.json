  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}/packages/examples",
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "runtimeExecutable": "npm",
      "args": [
        "run",
        "debug"
