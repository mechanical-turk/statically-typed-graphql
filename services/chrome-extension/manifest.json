{
  "name": "Whatsgood.dog",
  "version": "1.0",
  "description": "Here's your RSS feed fam",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "Whatsgood RSS"
  }
}