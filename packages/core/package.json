{
  "name": "@whatsgood/uniform-graphql",
  "private": false,
  "version": "0.2.1",
  "description": "Create code-first graphql backends in TypeScript with zero type-safety compromise",
  "repository": {
    "type": "git",
    "url": "https://github.com/whats-good/uniform-graphql"
  },
  "main": "build/index.js",
  "author": "Kerem Kazan",
  "license": "MIT",
  "keywords": [
    "graphql",
    "typescript",
    "statically",
    "static",
    "typed",
    "strongly",
    "strong",
    "compile",
    "time",
    "compiler"
  ],
  "scripts": {
    "clean": "rm -rf ./build",
    "prep": "npm run build && npm run propagate-readme",
    "propagate-readme": "cp ../../README.md ./README.md",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "build": "npm run clean && npm run rebuild",
    "rebuild": "tsc -p ./tsconfig.json",
    "debug": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "graphql": "^15.3.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.165",
    "apollo-server-express": "2.19.0",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "ts-node": "file:../../node_modules/ts-node",
    "tsd": "file:../../node_modules/tsd",
    "typescript": "file:../../node_modules/typescript"
  }
}